#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/robertlong/ember/pnpm-ember-workspaces/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/robertlong/ember/pnpm-ember-workspaces/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../node_modules/.pnpm/qunit@2.19.4/node_modules/qunit/bin/qunit.js" "$@"
else
  exec node  "$basedir/../../../node_modules/.pnpm/qunit@2.19.4/node_modules/qunit/bin/qunit.js" "$@"
fi
